<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.flower.entitles">
	
	<class name="User" table="t_user">
		<id name="id" column="id" >
			<generator class="native"></generator>
		</id>
		<property name="username" unique="true"></property>
		<property name="passWord"  ></property>
		<property name="realName" ></property>
		<property name="address"  ></property>
		<property name="photoNum" unique="true"></property>
		
		
		<!-- <many-to-one name="c" column="classId" class="com.hiberate.model.Class" cascade="save-update"></many-to-one> -->
	</class>

	

</hibernate-mapping>